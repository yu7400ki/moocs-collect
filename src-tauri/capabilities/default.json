{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "store:default",
    "dialog:default",
    "process:default",
    "process:allow-restart",
    "opener:allow-open-path",
    {
      "identifier": "opener:allow-open-path",
      "allow": [
        {
          "path": "**/*.pdf"
        }
      ]
    }
  ]
}
